{
    "name": "heptacom/shopware-platform-plugin-project",
    "description": "HEPTACOM Shopware 6 Plugin",
    "license": "proprietary",
    "type": "shopware-platform-plugin",
    "version": "0.1.0",
    "keywords": [
        "shopware",
        "ecommerce"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH",
            "email": "info@heptacom.de",
            "role": "Manufacturer"
        }
    ],
    "homepage": "https://www.heptacom.de/",
    "require": {
        "php": ">=8.2",
        "shopware/core": "^6.6"
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\ShopwarePlatform\\PluginName\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "label": {
            "de-DE": "Plugin name",
            "en-GB": "Plugin name"
        },
        "description": {
            "de-DE": "HEPTACOM Shopware 6 Plugin",
            "en-GB": "HEPTACOM Shopware 6 Plugin"
        },
        "shopware-plugin-class": "Heptacom\\ShopwarePlatform\\PluginName\\HeptacomShopwarePlatformPluginName",
        "manufacturerLink": {
            "de-DE": "https://www.heptacom.de/",
            "en-GB": "https://www.heptacom.de/"
        },
        "supportLink": {
            "de-DE": "https://www.heptacom.de/kontakt/",
            "en-GB": "https://www.heptacom.de/kontakt/"
        }
    }
}
