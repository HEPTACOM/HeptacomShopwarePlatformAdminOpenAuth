rules:
	- Heptacom\ShopwarePlatform\DevOps\PhpStan\Rule\ContractsHaveDocumentationRule
	- Heptacom\ShopwarePlatform\DevOps\PhpStan\Rule\ImplementationsMustBeFinalRule
	- Heptacom\ShopwarePlatform\DevOps\PhpStan\Rule\InterfacesHaveDocumentationRule
	- Shopware\Core\DevOps\StaticAnalyze\PHPStan\Rules\Deprecation\DeprecatedMethodsThrowDeprecationRule
parameters:
	level: 8
	checkMissingIterableValueType: false
	inferPrivatePropertyTypeFromConstructor: true
	paths:
		- ../dev-ops/bin/phpstan/src/
		- ../src/
	tmpDir: ../.build/phpstan/
	scanDirectories:
		- ../vendor/
